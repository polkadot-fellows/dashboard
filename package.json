{
  "name": "polkadot-fellows-dashboard",
  "private": "false",
  "version": "0.0.1",
  "contributors": [
    "Nikolaos Kontakis<wirednkod@gmail.com>"
  ],
  "description": "A dashboard about Polkadot Fellowship and everything around it",
  "type": "module",
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postinstall": "papi",
    "build:pages": "tsc && vite build --base '/dashboard/'",
    "deploy:pages": "pnpm run build:pages && gh-pages -d build",
    "deploy": "gh-pages -d build --base '/dashboard/'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && prettier --check .",
    "lint:fix": "eslint . --ext ts,tsx --fix && prettier --write .",
    "preview": "vite preview"
  },
  "polkadot-api": {
    "polkadot": {
      "outputFolder": "src/codegen",
      "wsUrl": "wss://polkadot-rpc.dwellir.com",
      "metadata": "polkadot.scale"
    },
    "collectives": {
      "outputFolder": "src/codegen",
      "wsUrl": "wss://polkadot-collectives-rpc.polkadot.io",
      "metadata": "collectives.scale"
    }
  },
  "dependencies": {
    "@mdx-js/react": "^3.0.1",
    "@mdx-js/rollup": "^3.0.1",
    "@polkadot-api/cli": "0.0.1-28172e65e6c4fb53198b9932ff624f8eef05fe68.1.0",
    "@polkadot-api/client": "0.0.1-28172e65e6c4fb53198b9932ff624f8eef05fe68.1.0",
    "@polkadot-api/legacy-polkadot-provider": "0.0.1-28172e65e6c4fb53198b9932ff624f8eef05fe68.1.0",
    "@polkadot-api/node-polkadot-provider": "0.0.1-28172e65e6c4fb53198b9932ff624f8eef05fe68.1.0",
    "@polkadot-api/substrate-client": "0.0.1-28172e65e6c4fb53198b9932ff624f8eef05fe68.1.0",
    "@polkadot-api/utils": "0.0.1-28172e65e6c4fb53198b9932ff624f8eef05fe68.1.0",
    "@polkadot-api/ws-provider": "0.0.1-28172e65e6c4fb53198b9932ff624f8eef05fe68.1.0",
    "@polkadot-ui/core": "0.0.1-alpha.2",
    "@polkadot-ui/react": "^0.5.3",
    "@polkadot-ui/utils": "0.0.1-alpha.2",
    "@substrate/connect": "^0.8.8",
    "antd": "^5.16.0",
    "buffer": "^6.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-icons": "^5.0.1",
    "react-loader-spinner": "^6.1.6",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
    "usehooks-ts": "^3.0.2"
  },
  "devDependencies": {
    "@polkadot-api/rollup-plugin-descriptor-treeshake": "0.0.1-e5b6c751cfb875da2542f1bd895e0be3aa95cd4a.1.0",
    "@types/mdx": "^2.0.11",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prefer-arrow-functions": "^3.3.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "sass": "^1.72.0",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vite-bundle-visualizer": "^1.1.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
